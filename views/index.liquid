{% render 'partials/head.liquid' %}

<h2>Members</h2>
<ul>
  {% for person in persons %}
    <li><a href="/detail/{{ person.id }}">{{ person.name }}</a></li>
  {% endfor %}
</ul>

    <section>
      <h2>Squads</h2>
      <ul>
        {% for squad in squads %}
          <li>Squad {{ squad.name }}</li>
        {% endfor %}
      </ul>
    </section>

{% render 'partials/foot.liquid', squads: squads %}